      if(SD14.length&&SIDEMTC[i]==sd14){CHKO(SD00.length,SD14[SD14.length-1]);VALI(7);xksi--;}//Kid's Chips
      if(SD15.length&&SIDEMTC[i]==sd15){VALI(7);}//Kid's Fruit
